{
  "name": "jerseyfm",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.18.0"
  },
  "description": "demo app link: https://insta-reels-one.vercel.app/",
  "main": "tailwind.config.js",
  "author": "Wizard of Hahz",
  "license": "ISC",
  "packageManager": "yarn@4.9.1",
  "scripts": {
    "start": "react-dotenv && react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true react-scripts build",
    "serve": "react-dotenv && serve build",
    "eject": "react-scripts eject",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,json}\""
  },
  "dependencies": {
    "@account-kit/core": "^4.31.0",
    "@account-kit/infra": "^4.31.0",
    "@account-kit/react": "^4.31.0",
    "@account-kit/signer": "^4.31.0",
    "@account-kit/smart-contracts": "^4.31.0",
    "@alchemy/aa-alchemy": "^3.19.0",
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.6",
    "@mui/material": "^5.16.6",
    "@tanstack/react-query": "^5.75.1",
    "@thirdweb-dev/react": "^4.9.4",
    "@thirdweb-dev/sdk": "^4.0.99",
    "@vercel/analytics": "^1.5.0",
    "ethers": "^6.13.7",
    "framer-motion": "^11.18.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dotenv": "^0.1.3",
    "react-icons": "^5.2.1",
    "react-intersection-observer": "^9.15.0",
    "react-router-dom": "^6.26.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.12",
    "thirdweb": "^5.96.7",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/react": "^18.2.40",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "autoprefixer": "^10.4.20",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "postcss": "^8.5.1",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "semi": true,
    "tabWidth": 2,
    "printWidth": 80,
    "singleQuote": false,
    "trailingComma": "all",
    "jsxSingleQuote": false,
    "bracketSpacing": true
  }
}
